<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">

            </xpath>
        </template>

        <template id="assets_frontend" inherit_id="web.assets_frontend">
            <xpath expr="." position="inside">


                <!-- Favicons -->
                <link href="/custom_web/static/src/img/favicon.png" rel="icon" />
                <link href="/custom_web/static/src/img/apple-touch-icon.png" rel="apple-touch-icon" />

                <!-- Fonts -->
                <link href="https://fonts.googleapis.com" rel="preconnect" />
                <link href="https://fonts.gstatic.com" rel="preconnect" />
                <link
                    href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&amp;family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap"
                    rel="stylesheet" />

                <!-- Bootstrap -->
                <link href="/custom_web/static/src/vendor/bootstrap/css/bootstrap.min.css"
                    rel="stylesheet" type="text/css" />
                <link href="/custom_web/static/src/vendor/bootstrap-icons/bootstrap-icons.css"
                    rel="stylesheet" type="text/css" />


                <!-- Main CSS File -->
                <link href="/custom_web/static/src/css/main.css" rel="stylesheet"
                    type="text/css" />
                <!-- Main JS File -->
                <script type="text/javascript"
                    src="/custom_web/static/src/js/main.js"></script>
                <!-- AOS Animation -->
                <link rel="stylesheet" type="text/css"
                    href="/custom_web/static/src/vendor/aos/aos.css" />
                <script type="text/javascript" src="/custom_web/static/src/vendor/aos/aos.js"></script>
                <!-- Typed.js -->
                <script type="text/javascript"
                    src="/custom_web/static/src/vendor/typed.js/typed.umd.js"></script>
                <!-- Pure Counter -->
                <script type="text/javascript"
                    src="/custom_web/static/src/vendor/purecounter/purecounter_vanilla.js"></script>
                <!-- Waypoints -->
                <script type="text/javascript"
                    src="/custom_web/static/src/vendor/waypoints/noframework.waypoints.js"></script>
                <!-- GLightbox -->
                <script type="text/javascript"
                    src="/custom_web/static/src/vendor/glightbox/js/glightbox.min.js"></script>
                <link href="/custom_web/static/src/vendor/glightbox/css/glightbox.min.css"
                    rel="stylesheet" type="text/css" />
                <!-- ImagesLoaded -->
                <script type="text/javascript"
                    src="/custom_web/static/src/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
                <!-- Isotope -->
                <script src="/custom_web/static/src/vendor/isotope-layout/isotope.pkgd.min.js"></script>
                <!-- Swiper -->
                <script src="/custom_web/static/src/vendor/swiper/swiper-bundle.min.js"></script>
                <link href="/custom_web/static/src/vendor/swiper/swiper-bundle.min.css"
                    rel="stylesheet" type="text/css" />
                <!-- Devicons -->
                <link rel="stylesheet" type='text/css'
                    href="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/devicon.min.css" />
            </xpath>
        </template>
    </data>
</odoo>